{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "rootDir": "."
  },
  "include": ["generated/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
